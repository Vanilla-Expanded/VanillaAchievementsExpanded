<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<AchievementsExpanded.AchievementDef>
		<defName>VAE_OnlyQOL</defName>
		<label>It's only QoL!</label>
		<description>Have 10 mods or more installed</description>
		
		<tracker Class="AchievementsExpanded.ModAmountTracker">
			<amountOfMods>10</amountOfMods>
		</tracker>
		<blockedUntilMonolithLevel>Waking</blockedUntilMonolithLevel>
		<texPath>Achievements/InForAPenny</texPath>
		<points>10</points>
	</AchievementsExpanded.AchievementDef>
	
	<AchievementsExpanded.AchievementDef>
		<defName>VAE_UnstableCells</defName>
		<label>Surely they won’t explode</label>
		<description>Collect 1 unstable power cell </description>
		
		<tracker Class="AchievementsExpanded.BuildingTracker">
			<def>UnstablePowerCell</def>
			<count>1</count>
		</tracker>
		
		<texPath>Achievements/ItAintRocketScience</texPath>
		<points>50</points>
	</AchievementsExpanded.AchievementDef>
	
	
</Defs>
